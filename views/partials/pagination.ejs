<nav class="my-5" aria-label="Page navigation example">
    <ul class="pagination justify-content-center">
        <li class="page-item <% if (!page.has_pre) { %>disabled<% } %>">
            <a class="page-link" href="/<%- page.category %>?page=<%- page.current_page - 1 %>" tabindex="-1">Previous</a>
        </li>
        <% for (i = 1; i < page.total_pages + 1; i++) { %>
            <li class="page-item <% if (page.current_page === i) { %>active<% } %>">
                <a class="page-link" href="/<%- page.category %><% if (page.category == '') { %>?<% } else { %>&<% } %>page=<%- i %>">
                    <%- i %>
                </a>
            </li>
            <% } %>
                <li class="page-item <% if (!page.has_next) { %>disabled<% } %>">
                    <a class="page-link" href="
                    /<%- page.category %><% if (page.category == '') { %>?<% } else { %>&<% } %>page=<%- page.current_page + 1 %>
                    ">Next</a>
                </li>
    </ul>
</nav>